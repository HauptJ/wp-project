{
    "name": "devgeniem/wp-project",
    "type": "project",
    "license": "MIT",
    "description": "Bedrock inspired WordPress template",
    "authors": [
        {
            "name": "Onni Hakala",
            "email": "onni.hakala@geniem.com",
            "homepage": "https://github.com/onnimonni",
            "role": "Project Lead Developer"
        },
        {
            "name": "Hannu Kumpula",
            "email": "hannu.kumpula@geniem.com",
            "homepage": "http://geniem.com",
            "role": "Project Manager"
        }
    ],
    "keywords": [
        "wordpress",
        "composer",
        "wp"
    ],
    "config": {
        "preferred-install": "dist"
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        "1": {
            "type": "composer",
            "url": "https://wp-languages.github.io"
        },
        "2": {
            "type": "git",
            "url": "git@github.com:devgeniem/ext-advanced-custom-fields-pro.git"
        },
        "3": {
            "type": "git",
            "url": "https://github.com/devgeniem/ext-polylang-pro.git"
        },
        "devgeniem/dustpress-component-post-list": {
            "type": "git",
            "url": "git@github.com:devgeniem/dustpress-component-post-list.git"
        },
        "dustpress-components": {
            "type": "git",
            "url": "git@github.com:devgeniem/dustpress-components.git"
        },
        "devgeniem/dustpress-component-link-list": {
            "type": "git",
            "url": "git@github.com:devgeniem/dustpress-component-link-list.git"
        }
    },
    "require": {
        "php": ">=7.0",
        "johnpbloch/wordpress": "^4.5.0",
        "vlucas/phpdotenv": "^2.0.1",
        "oscarotero/env": "^1.0",
        "composer/installers": "v1.0.12",
        "koodimonni/composer-dropin-installer": "^1.0",
        "koodimonni-language/core-fi": "*",
        "devgeniem/wp-safe-fast-and-clean-collection": "^1.0",
        "devgeniem/better-wp-db-error": "^0.1",
        "devgeniem/wp-noindex-testing-staging-robots": "^1.0",
        "robmorgan/phinx": "^0.5.3",
        "wpackagist-plugin/stream": "^3.2.0",
        "wpackagist-plugin/enhanced-media-library": "^2.4.4",
        "devgeniem/wp-redis-object-cache-dropin": "^1.3.4",
        "devgeniem/wp-seo-collection": "^1.0",
        "devgeniem/wp-redis-group-cache": "^2.0",
        "devgeniem/dustpress": "1.*",
        "devgeniem/dustpress-debugger": "1.*",
        "devgeniem/dustpress-js": "*",
        "devgeniem/ext-advanced-custom-fields-pro": "^5.1",
        "devgeniem/ext-polylang-pro": "^2.2",
        "devgeniem/acf-codifier": "^1.1",
        "devgeniem/dustpress-components": "^1.1.1",
        "devgeniem/dustpress-component-link-list": "dev-master as 1.1.0",
        "devgeniem/dustpress-component-post-list": "dev-master"
    },
    "require-dev": {
        "rarst/wps": ">=1.0.0"
    },
    "extra": {
        "installer-paths": {
            "web/app/mu-plugins/{$name}/": [
                "type:wordpress-muplugin",
                "rarst/wps"
            ],
            "web/app/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "web/app/themes/{$name}": [
                "type:wordpress-theme"
            ]
        },
        "dropin-paths": {
            "web/app/": [
                "type:wordpress-dropin"
            ],
            "web/app/languages/": [
                "vendor:koodimonni-language"
            ],
            "web/app/languages/plugins/": [
                "vendor:koodimonni-plugin-language"
            ],
            "web/app/languages/themes/": [
                "vendor:koodimonni-theme-language"
            ]
        },
        "wordpress-install-dir": "web/wp"
    }
}
